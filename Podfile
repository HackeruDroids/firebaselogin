# Uncomment the next line to define a global platform for your project 11+
platform :ios, '11.0'

target 'FirebaseDemos' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FirebaseDemos
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'

end
